{
    "name": "fsi/admin-security-bundle",
    "type": "symfony-bundle",
    "description": "Simple authentication & authorization mechanisms for fsi/admin-bundle",
    "keywords": ["admin", "panel", "symfony", "bundle", "fsi", "security"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=7.1",
        "fsi/admin-bundle" : "^3.0@dev",
        "symfony/dependency-injection": "^2.7|^3.0",
        "symfony/form": "^2.7|^3.0",
        "symfony/framework-bundle": "^2.7|^3.0",
        "symfony/security": "^2.6|^3.0",
        "symfony/security-bundle": "^2.6|^3.0",
        "symfony/swiftmailer-bundle": "^2.4|^3.0",
        "symfony/property-access": "^2.6|^3.0",
        "symfony/twig-bundle": "^2.7|^3.0",
        "symfony/validator": "^2.6|^3.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.5",
        "paragonie/random_compat": "^1.1"
    },
    "require-dev": {
        "symfony/dom-crawler": "^2.5|^3.0",
        "symfony/class-loader": "^2.4|^3.0",
        "symfony/monolog-bridge": "^2.4|^3.0",
        "symfony/monolog-bundle": "^2.6|^3.0",
        "symfony/routing": "^2.7|^3.0",
        "phpspec/phpspec": "^3.0",
        "phpspec/prophecy": "^1.7.2",
        "behat/behat": "^3.3",
        "behat/symfony2-extension": "^2.1",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-selenium2-driver": "^1.3",
        "sensiolabs/behat-page-object-extension": "^2.0",
        "bossa/phpspec2-expect": "^2.3",
        "fsi/datasource": "^2.0@dev",
        "fsi/datasource-bundle": "^2.0@dev"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Bundle\\AdminSecurityBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "2.0": "2.0-dev"
        }
    }
}
