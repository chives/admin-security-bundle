{
    "name": "fsi/admin-security-bundle",
    "type": "symfony-bundle",
    "description": "Purpose of this bundle is to provide simple authentication & authorization mechanisms for fsi/admin-bundle",
    "keywords": ["admin", "panel", "symfony2", "bundle", "fsi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "fsi/admin-bundle" : "~2.0@dev",
        "symfony/form": "^2.4",
        "symfony/framework-bundle": "^2.3",
        "symfony/security": "^2.6",
        "symfony/security-bundle": "^2.6",
        "symfony/swiftmailer-bundle": "^2.3.3",
        "symfony/twig-bundle": "^2.6",
        "symfony/validator": "^2.3",
        "doctrine/orm": "^2.4.6",
        "doctrine/doctrine-bundle": "^1.5",
        "paragonie/random_compat": "^1.1"
    },
    "require-dev": {
        "symfony/expression-language": "^2.4",
        "symfony/monolog-bundle": "^2.6",
        "friendsofsymfony/user-bundle": "1.3.*",
        "phpspec/phpspec": "^2.4",
        "behat/behat": "^3.0.15",
        "behat/symfony2-extension": "^2.1",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.1",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-selenium2-driver": "^1.2",
        "sensiolabs/behat-page-object-extension": "~2.0@dev",
        "bossa/phpspec2-expect": "^1.0",
        "symfony/class-loader": "^2.3"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Bundle\\AdminSecurityBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
